{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Main Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "console": "integratedTerminal",
      "showLog": true,
      "trace": "verbose",
      "logOutput": "rpc",
      "args": [],
      "env": {},
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
      "dlvFlags": [
        "--check-go-version=false"
      ]
    },
    {
      "name": "Debug Current File",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${file}",
      "console": "integratedTerminal",
      "showLog": true,
      "cwd": "${fileDirname}",
      "stopOnEntry": false,
      "dlvFlags": [
        "--check-go-version=false"
      ]
    },
    {
      "name": "Debug with Arguments",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "console": "integratedTerminal",
      "args": [
        "--port=9091",
        "--debug"
      ],
      "env": {
        "PORT": "9091",
        "DEBUG": "true"
      },
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
      "dlvFlags": [
        "--check-go-version=false"
      ]
    },
    {
      "name": "Attach to Process",
      "type": "go",
      "request": "attach",
      "mode": "local",
      "processId": "${command:pickProcess}",
      "showLog": true,
      "trace": "verbose"
    },
    {
      "name": "Connect to Remote Delve",
      "type": "go",
      "request": "attach",
      "mode": "remote",
      "remotePath": "${workspaceFolder}",
      "port": 2345,
      "host": "127.0.0.1",
      "showLog": true,
      "trace": "verbose"
    },
    {
      "name": "Debug Tests in Current File",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${file}",
      "console": "integratedTerminal",
      "showLog": true,
      "args": [
        "-test.v",
        "-test.run",
        ".*"
      ],
      "env": {},
      "cwd": "${fileDirname}"
    },
    {
      "name": "Debug All Tests in Package",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${fileDirname}",
      "console": "integratedTerminal",
      "showLog": true,
      "args": [
        "-test.v",
        "-test.cover"
      ],
      "env": {},
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Debug All Tests in Workspace",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${workspaceFolder}",
      "console": "integratedTerminal",
      "showLog": true,
      "args": [
        "-test.v",
        "-test.cover",
        "./..."
      ],
      "env": {},
      "cwd": "${workspaceFolder}"
    }
  ]
}